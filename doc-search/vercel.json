{
  "version": 2,
  "builds": [
    { "src": "src/app/backend/server.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { 
      "src": "/api/(users|search)/(.*)",
      "dest": "src/app/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}