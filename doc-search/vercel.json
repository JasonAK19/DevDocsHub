{
  "version": 2,
  "buildCommand": "next build",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://dev-docs-hub.vercel.app",
    "NODE_ENV": "production"
  }
}